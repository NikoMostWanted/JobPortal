            <a href="{{ url('answerRegion') }}?region=сумская область" style="text-decoration: none; color: blue">Сумская Область</a><br/>
            <a href="{{ url('answerRegion') }}?region=тернопольская область" style="text-decoration: none; color: blue">Тернопольская Область</a><br/>
            <a href="{{ url('answerRegion') }}?region=харьковская область" style="text-decoration: none; color: blue">Харьковская Область</a><br/>
            <a href="{{ url('answerRegion') }}?region=херсонская область" style="text-decoration: none; color: blue">Херсонская Область</a><br/>
            <a href="{{ url('answerRegion') }}?region=хмельницкая область" style="text-decoration: none; color: blue">Хмельницкая Область</a><br/>
            <a href="{{ url('answerRegion') }}?region=черкасская область" style="text-decoration: none; color: blue">Черкасская Область</a><br/>
            <a href="{{ url('answerRegion') }}?region=черниговская область" style="text-decoration: none; color: blue">Черниговская Область</a><br/>
            <a href="{{ url('answerRegion') }}?region=черновицкая область" style="text-decoration: none; color: blue">Черновицкая Область</a><br/>
        </div>
    </div>
    {% image '@AppBundle/Resources/public/images/footer_bg.gif'  output='/images/footer_bg.gif' %}
    <div id="footer" style="background-image:url({{ asset_url }});">
        {% endimage %}
        Copyright &copy;. All rights reserved. Design by <a href="https://vk.com/active_11">Neymar</a> and <a
                href="https://vk.com/id135256716"> Niko</a>
    </div>
{% endblock %}
